<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:property-placeholder ignore-unresolvable="true" system-properties-mode="OVERRIDE"/>
    <bean id="uniauthGrp" class="com.dianrong.platform.cfg.CfgGroup">
        <property name="cfgRootNodeName" value="/com/dianrong/cfg/1.0.0/uniauth"/>
        <property name="needKeepAlive" value="false"/>
    </bean>
    <context:annotation-config />
    <bean class="com.dianrong.common.uniauth.sharerw.facade.UARWFacade">
        <property name="uniWsEndpoint" value="#{uniauthGrp['uniauth_ws_endpoint']}" />
    </bean>

</beans>